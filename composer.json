{
    "name": "synapsestudios/api-template",
    "description": "Starting point for new projects",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/synapsestudios/api-template-base-library"
        }
    ],
    "require": {
        "synapsestudios/api-template-base-library": "~0.1.0",
        "silex/silex": "~1.1",
        "symfony/console": "~2.3.8",
        "symfony/security": "~2.3",
        "zendframework/zend-db": "~2.2.5",
        "bshaffer/oauth2-server-php": "v1.0",
        "bshaffer/oauth2-server-httpfoundation-bridge": "v1.0",
        "chrisboulton/php-resque": "v1.2",
        "mandrill/mandrill": "1.0.52",
        "psr/log": "v1.0.0",
        "monolog/monolog": "~1.7",
        "rollbar/rollbar": "0.6.3",
        "zendframework/zend-stdlib": "~2.2.5",
        "ircmaxell/password-compat": "dev-master",
        "guzzle/guzzle": "~3.7",
        "jdesrosiers/silex-cors-provider": "~0.1",
        "lusitanian/oauth": "~0.2",
        "mustache/mustache": "~2.5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "dev-master"
    },
    "autoload": {
        "psr-0": { "Synapse": "src/" }
    }
}
